{
  "productName": "electron-best-practice",
  "appId": "com.dg.cjj",
  "extraResources": [
    {
      "from": "main/server",
      "to": "main/server"
    },
    {
      "from": "./config",
      "to": "config"
    },
    {
      "from": "build_metadata.json",
      "to": "build_metadata.json"
    }
  ],
  "files": [
    "dist/"
  ],
  "win": {
    "icon": "resources/icon.ico",
    "target": [
      "portable"
    ]
  },
  "directories": {
    "output": "release"
  },
  "publish": {
    "provider": "generic",
    "url": "http://127.0.0.1:3000"
  }
}